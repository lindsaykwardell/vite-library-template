{
  "name": "vite-library-template",
  "private": false,
  "version": "0.0.1",
  "author": "Lindsay Wardell",
  "description": "Template for scaffolding a Vite-based library",
  "main": "dist/vite-library-template.es.js",
  "types": "dist/index.d.ts",
